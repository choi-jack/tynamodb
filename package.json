{
    "name": "tynamodb",
    "version": "0.0.0",
    "description": "An abstracted DynamoDB document client and expression dsl.",
    "keywords": [
        "aws",
        "document-client",
        "dsl",
        "dynamodb",
        "expression"
    ],
    "files": [
        "./dist"
    ],
    "type": "module",
    "exports": "./dist/index.js",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "lint": "eslint ./src",
        "test": "jest",
        "prebuild": "npm run clean",
        "build": "tsc -p ./tsconfig.build.json",
        "clean": "rimraf --glob ./dist ./*.tgz ./coverage",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "prepack": "npm run build"
    },
    "author": "choi-jack <choi.jaehyeok@outlook.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/choi-jack/tynamodb.git"
    },
    "dependencies": {
        "tslib": "^2.6.2"
    },
    "devDependencies": {
        "@aws-sdk/client-dynamodb": "^3.496.0",
        "@aws-sdk/lib-dynamodb": "^3.496.0",
        "@jest/globals": "^29.7.0",
        "@stylistic/eslint-plugin": "^1.5.4",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "conventional-changelog-cli": "^4.1.0",
        "eslint": "^8.55.0",
        "jest": "^29.7.0",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2"
    }
}
